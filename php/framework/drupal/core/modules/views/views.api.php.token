[["T_OPEN_TAG","<?php\n",1],["T_WHITESPACE","\n",2],["T_DOC_COMMENT","\/**\n * @file\n * Describes hooks and plugins provided by the Views module.\n *\/",3],["T_WHITESPACE","\n\n",6],["T_USE","use",8],["T_WHITESPACE"," ",8],["T_STRING","Drupal",8],["T_NS_SEPARATOR","\\",8],["T_STRING","Core",8],["T_NS_SEPARATOR","\\",8],["T_STRING","Language",8],["T_NS_SEPARATOR","\\",8],["T_STRING","LanguageInterface",8],";",["T_WHITESPACE","\n",8],["T_USE","use",9],["T_WHITESPACE"," ",9],["T_STRING","Drupal",9],["T_NS_SEPARATOR","\\",9],["T_STRING","views",9],["T_NS_SEPARATOR","\\",9],["T_STRING","Plugin",9],["T_NS_SEPARATOR","\\",9],["T_STRING","views",9],["T_NS_SEPARATOR","\\",9],["T_STRING","cache",9],["T_NS_SEPARATOR","\\",9],["T_STRING","CachePluginBase",9],";",["T_WHITESPACE","\n",9],["T_USE","use",10],["T_WHITESPACE"," ",10],["T_STRING","Drupal",10],["T_NS_SEPARATOR","\\",10],["T_STRING","views",10],["T_NS_SEPARATOR","\\",10],["T_STRING","Plugin",10],["T_NS_SEPARATOR","\\",10],["T_STRING","views",10],["T_NS_SEPARATOR","\\",10],["T_STRING","PluginBase",10],";",["T_WHITESPACE","\n",10],["T_USE","use",11],["T_WHITESPACE"," ",11],["T_STRING","Drupal",11],["T_NS_SEPARATOR","\\",11],["T_STRING","views",11],["T_NS_SEPARATOR","\\",11],["T_STRING","ViewExecutable",11],";",["T_WHITESPACE","\n\n",11],["T_DOC_COMMENT","\/**\n * @defgroup views_overview Views overview\n * @{\n * Overview of the Views module API\n *\n * The Views module is a generalized query and display engine, which can be used\n * to make views (formatted lists, grids, feeds, and other output) of items\n * (often entities, but can be other types of data). Developers can interact\n * with Views in several ways:\n * - Provide plugins: Views plugins govern nearly every aspect of views,\n *   including querying (sorting, filtering, etc.) and display (at several\n *   levels of granularity, ranging from the entire view to the details of a\n *   field). See the @link views_plugins Views plugins topic @endlink for\n *   more information.\n * - Provide data: Data types can be provided to Views by implementing\n *   hook_views_data(), and data types provided by other modules can be altered\n *   by implementing hook_views_data_alter(). To provide views data for an\n *   entity, create a class implementing\n *   \\Drupal\\views\\EntityViewsDataInterface and reference this in the\n *   \"views_data\" annotation in the entity class. You can autogenerate big parts\n *   of the integration if you extend the \\Drupal\\views\\EntityViewsData base\n *   class. See the @link entity_api Entity API topic @endlink for more\n *   information about entities.\n * - Implement hooks: A few operations in Views can be influenced by hooks.\n *   See the @link views_hooks Views hooks topic @endlink for a list.\n * - Theming: See the @link views_templates Views templates topic @endlink\n *   for more information.\n *\n * @see \\Drupal\\views\\ViewExecutable\n * @see \\Drupal\\views\\Views\n * @}\n *\/",13],["T_WHITESPACE","\n\n",44],["T_DOC_COMMENT","\/**\n * @defgroup views_plugins Views plugins\n * @{\n * Overview of views plugins\n *\n * Views plugins are objects that are used to build and render the view.\n * See individual views plugin topics for more information about the\n * specifics of each plugin type, and the\n * @link plugin_api Plugin API topic @endlink for more information about\n * plugins in general.\n *\n * Some Views plugins are known as handlers. Handler plugins help build the\n * view query object: filtering, contextual filtering, sorting, relationships,\n * etc.\n *\n * @todo Document specific options on the appropriate plugin base classes.\n * @todo Add examples.\n *\n * @ingroup views_overview\n * @see \\Drupal\\views\\Plugin\\views\\PluginBase\n * @see \\Drupal\\views\\Plugin\\views\\HandlerBase\n * @see plugin_api\n * @see annotation\n * @}\n *\/",46],["T_WHITESPACE","\n\n",70],["T_DOC_COMMENT","\/**\n * @defgroup views_hooks Views hooks\n * @{\n * Hooks that allow other modules to implement the Views API.\n * @ingroup views_overview\n * @}\n *\/",72],["T_WHITESPACE","\n\n",78],["T_DOC_COMMENT","\/**\n * @addtogroup hooks\n * @{\n *\/",80],["T_WHITESPACE","\n\n",83],["T_DOC_COMMENT","\/**\n * Analyze a view to provide warnings about its configuration.\n *\n * @param \\Drupal\\views\\ViewExecutable $view\n *   The view being executed.\n *\n * @return array\n *   Array of warning messages built by Analyzer::formatMessage to be displayed\n *   to the user following analysis of the view.\n *\/",85],["T_WHITESPACE","\n",94],["T_FUNCTION","function",95],["T_WHITESPACE"," ",95],["T_STRING","hook_views_analyze",95],"(",["T_STRING","Drupal",95],["T_NS_SEPARATOR","\\",95],["T_STRING","views",95],["T_NS_SEPARATOR","\\",95],["T_STRING","ViewExecutable",95],["T_WHITESPACE"," ",95],["T_VARIABLE","$view",95],")",["T_WHITESPACE"," ",95],"{",["T_WHITESPACE","\n  ",95],["T_VARIABLE","$messages",96],["T_WHITESPACE"," ",96],"=",["T_WHITESPACE"," ",96],["T_ARRAY","array",96],"(",")",";",["T_WHITESPACE","\n\n  ",96],["T_IF","if",98],["T_WHITESPACE"," ",98],"(",["T_VARIABLE","$view",98],["T_OBJECT_OPERATOR","->",98],["T_STRING","display_handler",98],["T_OBJECT_OPERATOR","->",98],["T_STRING","options",98],"[",["T_CONSTANT_ENCAPSED_STRING","'pager'",98],"]","[",["T_CONSTANT_ENCAPSED_STRING","'type'",98],"]",["T_WHITESPACE"," ",98],["T_IS_EQUAL","==",98],["T_WHITESPACE"," ",98],["T_CONSTANT_ENCAPSED_STRING","'none'",98],")",["T_WHITESPACE"," ",98],"{",["T_WHITESPACE","\n    ",98],["T_VARIABLE","$messages",99],"[","]",["T_WHITESPACE"," ",99],"=",["T_WHITESPACE"," ",99],["T_STRING","Drupal",99],["T_NS_SEPARATOR","\\",99],["T_STRING","views",99],["T_NS_SEPARATOR","\\",99],["T_STRING","Analyzer",99],["T_DOUBLE_COLON","::",99],["T_STRING","formatMessage",99],"(",["T_STRING","t",99],"(",["T_CONSTANT_ENCAPSED_STRING","'This view has no pager. This could cause performance issues when the view contains many items.'",99],")",",",["T_WHITESPACE"," ",99],["T_CONSTANT_ENCAPSED_STRING","'warning'",99],")",";",["T_WHITESPACE","\n  ",99],"}",["T_WHITESPACE","\n\n  ",100],["T_RETURN","return",102],["T_WHITESPACE"," ",102],["T_VARIABLE","$messages",102],";",["T_WHITESPACE","\n",102],"}",["T_WHITESPACE","\n\n",103],["T_DOC_COMMENT","\/**\n * Describe data tables and fields (or the equivalent) to Views.\n *\n * The table and fields are processed in Views using various plugins. See\n * the @link views_plugins Views plugins topic @endlink for more information.\n *\n * To provide views data for an entity, instead of implementing this hook,\n * create a class implementing \\Drupal\\views\\EntityViewsDataInterface and\n * reference this in the \"views\" annotation in the entity class. The return\n * value of the getViewsData() method on the interface is the same as this hook,\n * and base class in \\Drupal\\views\\EntityViewsData will take care of adding the\n * basic Views tables and fields for your entity. See the\n * @link entity_api Entity API topic @endlink for more information about\n * entities.\n *\n * The data described with this hook is fetched and retrieved by\n * \\Drupal\\views\\Views::viewsData()->get().\n *\n * @return array\n *   An associative array describing the structure of database tables and fields\n *   (and their equivalents) provided for use in Views. At the outermost level,\n *   the keys are the names used internally by Views for the tables (usually the\n *   actual table name). Each table's array describes the table itself, how to\n *   join to other tables, and the fields that are part of the table. The sample\n *   function body provides documentation of the details.\n *\n * @see hook_views_data_alter()\n *\/",105],["T_WHITESPACE","\n",132],["T_FUNCTION","function",133],["T_WHITESPACE"," ",133],["T_STRING","hook_views_data",133],"(",")",["T_WHITESPACE"," ",133],"{",["T_WHITESPACE","\n  ",133],["T_COMMENT","\/\/ This example describes how to write hook_views_data() for a table defined\n",134],["T_WHITESPACE","  ",135],["T_COMMENT","\/\/ like this:\n",135],["T_WHITESPACE","  ",136],["T_COMMENT","\/\/ CREATE TABLE example_table (\n",136],["T_WHITESPACE","  ",137],["T_COMMENT","\/\/   nid INT(11) NOT NULL         COMMENT 'Primary key: {node}.nid.',\n",137],["T_WHITESPACE","  ",138],["T_COMMENT","\/\/   plain_text_field VARCHAR(32) COMMENT 'Just a plain text field.',\n",138],["T_WHITESPACE","  ",139],["T_COMMENT","\/\/   numeric_field INT(11)        COMMENT 'Just a numeric field.',\n",139],["T_WHITESPACE","  ",140],["T_COMMENT","\/\/   boolean_field INT(1)         COMMENT 'Just an on\/off field.',\n",140],["T_WHITESPACE","  ",141],["T_COMMENT","\/\/   timestamp_field INT(8)       COMMENT 'Just a timestamp field.',\n",141],["T_WHITESPACE","  ",142],["T_COMMENT","\/\/   langcode VARCHAR(12)         COMMENT 'Language code field.',\n",142],["T_WHITESPACE","  ",143],["T_COMMENT","\/\/   PRIMARY KEY(nid)\n",143],["T_WHITESPACE","  ",144],["T_COMMENT","\/\/ );\n",144],["T_WHITESPACE","\n  ",145],["T_COMMENT","\/\/ Define the return array.\n",146],["T_WHITESPACE","  ",147],["T_VARIABLE","$data",147],["T_WHITESPACE"," ",147],"=",["T_WHITESPACE"," ",147],["T_ARRAY","array",147],"(",")",";",["T_WHITESPACE","\n\n  ",147],["T_COMMENT","\/\/ The outermost keys of $data are Views table names, which should usually\n",149],["T_WHITESPACE","  ",150],["T_COMMENT","\/\/ be the same as the hook_schema() table names.\n",150],["T_WHITESPACE","  ",151],["T_VARIABLE","$data",151],"[",["T_CONSTANT_ENCAPSED_STRING","'example_table'",151],"]",["T_WHITESPACE"," ",151],"=",["T_WHITESPACE"," ",151],["T_ARRAY","array",151],"(",")",";",["T_WHITESPACE","\n\n  ",151],["T_COMMENT","\/\/ The value corresponding to key 'table' gives properties of the table\n",153],["T_WHITESPACE","  ",154],["T_COMMENT","\/\/ itself.\n",154],["T_WHITESPACE","  ",155],["T_VARIABLE","$data",155],"[",["T_CONSTANT_ENCAPSED_STRING","'example_table'",155],"]","[",["T_CONSTANT_ENCAPSED_STRING","'table'",155],"]",["T_WHITESPACE"," ",155],"=",["T_WHITESPACE"," ",155],["T_ARRAY","array",155],"(",")",";",["T_WHITESPACE","\n\n  ",155],["T_COMMENT","\/\/ Within 'table', the value of 'group' (translated string) is used as a\n",157],["T_WHITESPACE","  ",158],["T_COMMENT","\/\/ prefix in Views UI for this table's fields, filters, etc. When adding\n",158],["T_WHITESPACE","  ",159],["T_COMMENT","\/\/ a field, filter, etc. you can also filter by the group.\n",159],["T_WHITESPACE","  ",160],["T_VARIABLE","$data",160],"[",["T_CONSTANT_ENCAPSED_STRING","'example_table'",160],"]","[",["T_CONSTANT_ENCAPSED_STRING","'table'",160],"]","[",["T_CONSTANT_ENCAPSED_STRING","'group'",160],"]",["T_WHITESPACE"," ",160],"=",["T_WHITESPACE"," ",160],["T_STRING","t",160],"(",["T_CONSTANT_ENCAPSED_STRING","'Example table'",160],")",";",["T_WHITESPACE","\n\n  ",160],["T_COMMENT","\/\/ Within 'table', the value of 'provider' is the module that provides schema\n",162],["T_WHITESPACE","  ",163],["T_COMMENT","\/\/ or the entity type that causes the table to exist. Setting this ensures\n",163],["T_WHITESPACE","  ",164],["T_COMMENT","\/\/ that views have the correct dependencies. This is automatically set to the\n",164],["T_WHITESPACE","  ",165],["T_COMMENT","\/\/ module that implements hook_views_data().\n",165],["T_WHITESPACE","  ",166],["T_VARIABLE","$data",166],"[",["T_CONSTANT_ENCAPSED_STRING","'example_table'",166],"]","[",["T_CONSTANT_ENCAPSED_STRING","'table'",166],"]","[",["T_CONSTANT_ENCAPSED_STRING","'provider'",166],"]",["T_WHITESPACE"," ",166],"=",["T_WHITESPACE"," ",166],["T_CONSTANT_ENCAPSED_STRING","'example_module'",166],";",["T_WHITESPACE","\n\n  ",166],["T_COMMENT","\/\/ Some tables are \"base\" tables, meaning that they can be the base tables\n",168],["T_WHITESPACE","  ",169],["T_COMMENT","\/\/ for views. Non-base tables can only be brought in via relationships in\n",169],["T_WHITESPACE","  ",170],["T_COMMENT","\/\/ views based on other tables. To define a table to be a base table, add\n",170],["T_WHITESPACE","  ",171],["T_COMMENT","\/\/ key 'base' to the 'table' array:\n",171],["T_WHITESPACE","  ",172],["T_VARIABLE","$data",172],"[",["T_CONSTANT_ENCAPSED_STRING","'example_table'",172],"]","[",["T_CONSTANT_ENCAPSED_STRING","'table'",172],"]","[",["T_CONSTANT_ENCAPSED_STRING","'base'",172],"]",["T_WHITESPACE"," ",172],"=",["T_WHITESPACE"," ",172],["T_ARRAY","array",172],"(",["T_WHITESPACE","\n    ",172],["T_COMMENT","\/\/ Identifier (primary) field in this table for Views.\n",173],["T_WHITESPACE","    ",174],["T_CONSTANT_ENCAPSED_STRING","'field'",174],["T_WHITESPACE"," ",174],["T_DOUBLE_ARROW","=>",174],["T_WHITESPACE"," ",174],["T_CONSTANT_ENCAPSED_STRING","'nid'",174],",",["T_WHITESPACE","\n    ",174],["T_COMMENT","\/\/ Label in the UI.\n",175],["T_WHITESPACE","    ",176],["T_CONSTANT_ENCAPSED_STRING","'title'",176],["T_WHITESPACE"," ",176],["T_DOUBLE_ARROW","=>",176],["T_WHITESPACE"," ",176],["T_STRING","t",176],"(",["T_CONSTANT_ENCAPSED_STRING","'Example table'",176],")",",",["T_WHITESPACE","\n    ",176],["T_COMMENT","\/\/ Longer description in the UI. Required.\n",177],["T_WHITESPACE","    ",178],["T_CONSTANT_ENCAPSED_STRING","'help'",178],["T_WHITESPACE"," ",178],["T_DOUBLE_ARROW","=>",178],["T_WHITESPACE"," ",178],["T_STRING","t",178],"(",["T_CONSTANT_ENCAPSED_STRING","'Example table contains example content and can be related to nodes.'",178],")",",",["T_WHITESPACE","\n    ",178],["T_CONSTANT_ENCAPSED_STRING","'weight'",179],["T_WHITESPACE"," ",179],["T_DOUBLE_ARROW","=>",179],["T_WHITESPACE"," ",179],"-",["T_LNUMBER","10",179],",",["T_WHITESPACE","\n  ",179],")",";",["T_WHITESPACE","\n\n  ",180],["T_COMMENT","\/\/ Some tables have an implicit, automatic relationship to other tables,\n",182],["T_WHITESPACE","  ",183],["T_COMMENT","\/\/ meaning that when the other table is available in a view (either as the\n",183],["T_WHITESPACE","  ",184],["T_COMMENT","\/\/ base table or through a relationship), this table's fields, filters, etc.\n",184],["T_WHITESPACE","  ",185],["T_COMMENT","\/\/ are automatically made available without having to add an additional\n",185],["T_WHITESPACE","  ",186],["T_COMMENT","\/\/ relationship. To define an implicit relationship that will make your\n",186],["T_WHITESPACE","  ",187],["T_COMMENT","\/\/ table automatically available when another table is present, add a 'join'\n",187],["T_WHITESPACE","  ",188],["T_COMMENT","\/\/ section to your 'table' section. Note that it is usually only a good idea\n",188],["T_WHITESPACE","  ",189],["T_COMMENT","\/\/ to do this for one-to-one joins, because otherwise your automatic join\n",189],["T_WHITESPACE","  ",190],["T_COMMENT","\/\/ will add more rows to the view. It is also not a good idea to do this if\n",190],["T_WHITESPACE","  ",191],["T_COMMENT","\/\/ most views won't need your table -- if that is the case, define a\n",191],["T_WHITESPACE","  ",192],["T_COMMENT","\/\/ relationship instead (see below).\n",192],["T_WHITESPACE","  ",193],["T_COMMENT","\/\/\n",193],["T_WHITESPACE","  ",194],["T_COMMENT","\/\/ If you've decided an automatic join is a good idea, here's how to do it;\n",194],["T_WHITESPACE","  ",195],["T_COMMENT","\/\/ the resulting SQL query will look something like this:\n",195],["T_WHITESPACE","  ",196],["T_COMMENT","\/\/   ... FROM example_table et ... JOIN node_field_data nfd\n",196],["T_WHITESPACE","  ",197],["T_COMMENT","\/\/   ON et.nid = nfd.nid AND ('extra' clauses will be here) ...\n",197],["T_WHITESPACE","  ",198],["T_COMMENT","\/\/ although the table aliases will be different.\n",198],["T_WHITESPACE","  ",199],["T_VARIABLE","$data",199],"[",["T_CONSTANT_ENCAPSED_STRING","'example_table'",199],"]","[",["T_CONSTANT_ENCAPSED_STRING","'table'",199],"]","[",["T_CONSTANT_ENCAPSED_STRING","'join'",199],"]",["T_WHITESPACE"," ",199],"=",["T_WHITESPACE"," ",199],["T_ARRAY","array",199],"(",["T_WHITESPACE","\n    ",199],["T_COMMENT","\/\/ Within the 'join' section, list one or more tables to automatically\n",200],["T_WHITESPACE","    ",201],["T_COMMENT","\/\/ join to. I