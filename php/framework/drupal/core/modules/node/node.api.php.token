[["T_OPEN_TAG","<?php\n",1],["T_WHITESPACE","\n",2],["T_DOC_COMMENT","\/**\n * @file\n * Hooks specific to the Node module.\n *\/",3],["T_WHITESPACE","\n\n",6],["T_USE","use",8],["T_WHITESPACE"," ",8],["T_STRING","Drupal",8],["T_NS_SEPARATOR","\\",8],["T_STRING","node",8],["T_NS_SEPARATOR","\\",8],["T_STRING","NodeInterface",8],";",["T_WHITESPACE","\n",8],["T_USE","use",9],["T_WHITESPACE"," ",9],["T_STRING","Drupal",9],["T_NS_SEPARATOR","\\",9],["T_STRING","Component",9],["T_NS_SEPARATOR","\\",9],["T_STRING","Utility",9],["T_NS_SEPARATOR","\\",9],["T_STRING","Html",9],";",["T_WHITESPACE","\n",9],["T_USE","use",10],["T_WHITESPACE"," ",10],["T_STRING","Drupal",10],["T_NS_SEPARATOR","\\",10],["T_STRING","Component",10],["T_NS_SEPARATOR","\\",10],["T_STRING","Utility",10],["T_NS_SEPARATOR","\\",10],["T_STRING","Xss",10],";",["T_WHITESPACE","\n",10],["T_USE","use",11],["T_WHITESPACE"," ",11],["T_STRING","Drupal",11],["T_NS_SEPARATOR","\\",11],["T_STRING","Core",11],["T_NS_SEPARATOR","\\",11],["T_STRING","Access",11],["T_NS_SEPARATOR","\\",11],["T_STRING","AccessResult",11],";",["T_WHITESPACE","\n\n",11],["T_DOC_COMMENT","\/**\n * @addtogroup hooks\n * @{\n *\/",13],["T_WHITESPACE","\n\n",16],["T_DOC_COMMENT","\/**\n * Inform the node access system what permissions the user has.\n *\n * This hook is for implementation by node access modules. In this hook,\n * the module grants a user different \"grant IDs\" within one or more\n * \"realms\". In hook_node_access_records(), the realms and grant IDs are\n * associated with permission to view, edit, and delete individual nodes.\n *\n * The realms and grant IDs can be arbitrarily defined by your node access\n * module; it is common to use role IDs as grant IDs, but that is not required.\n * Your module could instead maintain its own list of users, where each list has\n * an ID. In that case, the return value of this hook would be an array of the\n * list IDs that this user is a member of.\n *\n * A node access module may implement as many realms as necessary to properly\n * define the access privileges for the nodes. Note that the system makes no\n * distinction between published and unpublished nodes. It is the module's\n * responsibility to provide appropriate realms to limit access to unpublished\n * content.\n *\n * Node access records are stored in the {node_access} table and define which\n * grants are required to access a node. There is a special case for the view\n * operation -- a record with node ID 0 corresponds to a \"view all\" grant for\n * the realm and grant ID of that record. If there are no node access modules\n * enabled, the core node module adds a node ID 0 record for realm 'all'. Node\n * access modules can also grant \"view all\" permission on their custom realms;\n * for example, a module could create a record in {node_access} with:\n * @code\n * $record = array(\n *   'nid' => 0,\n *   'gid' => 888,\n *   'realm' => 'example_realm',\n *   'grant_view' => 1,\n *   'grant_update' => 0,\n *   'grant_delete' => 0,\n * );\n * db_insert('node_access')->fields($record)->execute();\n * @endcode\n * And then in its hook_node_grants() implementation, it would need to return:\n * @code\n * if ($op == 'view') {\n *   $grants['example_realm'] = array(888);\n * }\n * @endcode\n * If you decide to do this, be aware that the node_access_rebuild() function\n * will erase any node ID 0 entry when it is called, so you will need to make\n * sure to restore your {node_access} record after node_access_rebuild() is\n * called.\n *\n * For a detailed example, see node_access_example.module.\n *\n * @param \\Drupal\\Core\\Session\\AccountInterface $account\n *   The account object whose grants are requested.\n * @param string $op\n *   The node operation to be performed, such as 'view', 'update', or 'delete'.\n *\n * @return array\n *   An array whose keys are \"realms\" of grants, and whose values are arrays of\n *   the grant IDs within this realm that this user is being granted.\n *\n * @see node_access_view_all_nodes()\n * @see node_access_rebuild()\n * @ingroup node_access\n *\/",18],["T_WHITESPACE","\n",81],["T_FUNCTION","function",82],["T_WHITESPACE"," ",82],["T_STRING","hook_node_grants",82],"(",["T_NS_SEPARATOR","\\",82],["T_STRING","Drupal",82],["T_NS_SEPARATOR","\\",82],["T_STRING","Core",82],["T_NS_SEPARATOR","\\",82],["T_STRING","Session",82],["T_NS_SEPARATOR","\\",82],["T_STRING","AccountInterface",82],["T_WHITESPACE"," ",82],["T_VARIABLE","$account",82],",",["T_WHITESPACE"," ",82],["T_VARIABLE","$op",82],")",["T_WHITESPACE"," ",82],"{",["T_WHITESPACE","\n  ",82],["T_IF","if",83],["T_WHITESPACE"," ",83],"(",["T_VARIABLE","$account",83],["T_OBJECT_OPERATOR","->",83],["T_STRING","hasPermission",83],"(",["T_CONSTANT_ENCAPSED_STRING","'access private content'",83],")",")",["T_WHITESPACE"," ",83],"{",["T_WHITESPACE","\n    ",83],["T_VARIABLE","$grants",84],"[",["T_CONSTANT_ENCAPSED_STRING","'example'",84],"]",["T_WHITESPACE"," ",84],"=",["T_WHITESPACE"," ",84],["T_ARRAY","array",84],"(",["T_LNUMBER","1",84],")",";",["T_WHITESPACE","\n  ",84],"}",["T_WHITESPACE","\n  ",85],["T_IF","if",86],["T_WHITESPACE"," ",86],"(",["T_VARIABLE","$account",86],["T_OBJECT_OPERATOR","->",86],["T_STRING","id",86],"(",")",")",["T_WHITESPACE"," ",86],"{",["T_WHITESPACE","\n    ",86],["T_VARIABLE","$grants",87],"[",["T_CONSTANT_ENCAPSED_STRING","'example_author'",87],"]",["T_WHITESPACE"," ",87],"=",["T_WHITESPACE"," ",87],["T_ARRAY","array",87],"(",["T_VARIABLE","$account",87],["T_OBJECT_OPERATOR","->",87],["T_STRING","id",87],"(",")",")",";",["T_WHITESPACE","\n  ",87],"}",["T_WHITESPACE","\n  ",88],["T_RETURN","return",89],["T_WHITESPACE"," ",89],["T_VARIABLE","$grants",89],";",["T_WHITESPACE","\n",89],"}",["T_WHITESPACE","\n\n",90],["T_DOC_COMMENT","\/**\n * Set permissions for a node to be written to the database.\n *\n * When a node is saved, a module implementing hook_node_access_records() will\n * be asked if it is interested in the access permissions for a node. If it is\n * interested, it must respond with an array of permissions arrays for that\n * node.\n *\n * Node access grants apply regardless of the published or unpublished status\n * of the node. Implementations must make sure not to grant access to\n * unpublished nodes if they don't want to change the standard access control\n * behavior. Your module may need to create a separate access realm to handle\n * access to unpublished nodes.\n *\n * Note that the grant values in the return value from your hook must be\n * integers and not boolean TRUE and FALSE.\n *\n * Each permissions item in the array is an array with the following elements:\n * - 'realm': The name of a realm that the module has defined in\n *   hook_node_grants().\n * - 'gid': A 'grant ID' from hook_node_grants().\n * - 'grant_view': If set to 1 a user that has been identified as a member\n *   of this gid within this realm can view this node. This should usually be\n *   set to $node->isPublished(). Failure to do so may expose unpublished content\n *   to some users.\n * - 'grant_update': If set to 1 a user that has been identified as a member\n *   of this gid within this realm can edit this node.\n * - 'grant_delete': If set to 1 a user that has been identified as a member\n *   of this gid within this realm can delete this node.\n * - langcode: (optional) The language code of a specific translation of the\n *   node, if any. Modules may add this key to grant different access to\n *   different translations of a node, such that (e.g.) a particular group is\n *   granted access to edit the Catalan version of the node, but not the\n *   Hungarian version. If no value is provided, the langcode is set\n *   automatically from the $node parameter and the node's original language (if\n *   specified) is used as a fallback. Only specify multiple grant records with\n *   different languages for a node if the site has those languages configured.\n *\n * A \"deny all\" grant may be used to deny all access to a particular node or\n * node translation:\n * @code\n * $grants[] = array(\n *   'realm' => 'all',\n *   'gid' => 0,\n *   'grant_view' => 0,\n *   'grant_update' => 0,\n *   'grant_delete' => 0,\n *   'langcode' => 'ca',\n * );\n * @endcode\n * Note that another module node access module could override this by granting\n * access to one or more nodes, since grants are additive. To enforce that\n * access is denied in a particular case, use hook_node_access_records_alter().\n * Also note that a deny all is not written to the database; denies are\n * implicit.\n *\n * @param \\Drupal\\node\\NodeInterface $node\n *   The node that has just been saved.\n *\n * @return\n *   An array of grants as defined above.\n *\n * @see hook_node_access_records_alter()\n * @ingroup node_access\n *\/",92],["T_WHITESPACE","\n",156],["T_FUNCTION","function",157],["T_WHITESPACE"," ",157],["T_STRING","hook_node_access_records",157],"(",["T_NS_SEPARATOR","\\",157],["T_STRING","Drupal",157],["T_NS_SEPARATOR","\\",157],["T_STRING","node",157],["T_NS_SEPARATOR","\\",157],["T_STRING","NodeInterface",157],["T_WHITESPACE"," ",157],["T_VARIABLE","$node",157],")",["T_WHITESPACE"," ",157],"{",["T_WHITESPACE","\n  ",157],["T_COMMENT","\/\/ We only care about the node if it has been marked private. If not, it is\n",158],["T_WHITESPACE","  ",159],["T_COMMENT","\/\/ treated just like any other node and we completely ignore it.\n",159],["T_WHITESPACE","  ",160],["T_IF","if",160],["T_WHITESPACE"," ",160],"(",["T_VARIABLE","$node",160],["T_OBJECT_OPERATOR","->",160],["T_STRING","private",160],["T_OBJECT_OPERATOR","->",160],["T_STRING","value",160],")",["T_WHITESPACE"," ",160],"{",["T_WHITESPACE","\n    ",160],["T_VARIABLE","$grants",161],["T_WHITESPACE"," ",161],"=",["T_WHITESPACE"," ",161],["T_ARRAY","array",161],"(",")",";",["T_WHITESPACE","\n    ",161],["T_COMMENT","\/\/ Only published Catalan translations of private nodes should be viewable\n",162],["T_WHITESPACE","    ",163],["T_COMMENT","\/\/ to all users. If we fail to check $node->isPublished(), all users would be able\n",163],["T_WHITESPACE","    ",164],["T_COMMENT","\/\/ to view an unpublished node.\n",164],["T_WHITESPACE","    ",165],["T_IF","if",165],["T_WHITESPACE"," ",165],"(",["T_VARIABLE","$node",165],["T_OBJECT_OPERATOR","->",165],["T_STRING","isPublished",165],"(",")",")",["T_WHITESPACE"," ",165],"{",["T_WHITESPACE","\n      ",165],["T_VARIABLE","$grants",166],"[","]",["T_WHITESPACE"," ",166],"=",["T_WHITESPACE"," ",166],["T_ARRAY","array",166],"(",["T_WHITESPACE","\n        ",166],["T_CONSTANT_ENCAPSED_STRING","'realm'",167],["T_WHITESPACE"," ",167],["T_DOUBLE_ARROW","=>",167],["T_WHITESPACE"," ",167],["T_CONSTANT_ENCAPSED_STRING","'example'",167],",",["T_WHITESPACE","\n        ",167],["T_CONSTANT_ENCAPSED_STRING","'gid'",168],["T_WHITESPACE"," ",168],["T_DOUBLE_ARROW","=>",168],["T_WHITESPACE"," ",168],["T_LNUMBER","1",168],",",["T_WHITESPACE","\n        ",168],["T_CONSTANT_ENCAPSED_STRING","'grant_view'",169],["T_WHITESPACE"," ",169],["T_DOUBLE_ARROW","=>",169],["T_WHITESPACE"," ",169],["T_LNUMBER","1",169],",",["T_WHITESPACE","\n        ",169],["T_CONSTANT_ENCAPSED_STRING","'grant_update'",170],["T_WHITESPACE"," ",170],["T_DOUBLE_ARROW","=>",170],["T_WHITESPACE"," ",170],["T_LNUMBER","0",170],",",["T_WHITESPACE","\n        ",170],["T_CONSTANT_ENCAPSED_STRING","'grant_delete'",171],["T_WHITESPACE"," ",171],["T_DOUBLE_ARROW","=>",171],["T_WHITESPACE"," ",171],["T_LNUMBER","0",171],",",["T_WHITESPACE","\n        ",171],["T_CONSTANT_ENCAPSED_STRING","'langcode'",172],["T_WHITESPACE"," ",172],["T_DOUBLE_ARROW","=>",172],["T_WHITESPACE"," ",172],["T_CONSTANT_ENCAPSED_STRING","'ca'",172],["T_WHITESPACE","\n      ",172],")",";",["T_WHITESPACE","\n    ",173],"}",["T_WHITESPACE","\n    ",174],["T_COMMENT","\/\/ For the example_author array, the GID is equivalent to a UID, which\n",175],["T_WHITESPACE","    ",176],["T_COMMENT","\/\/ means there are many groups of just 1 user.\n",176],["T_WHITESPACE","    ",177],["T_COMMENT","\/\/ Note that an author can always view his or her nodes, even if they\n",177],["T_WHITESPACE","    ",178],["T_COMMENT","\/\/ have status unpublished.\n",178],["T_WHITESPACE","    ",179],["T_IF","if",179],["T_WHITESPACE"," ",179],"(",["T_VARIABLE","$node",179],["T_OBJECT_OPERATOR","->",179],["T_STRING","getOwnerId",179],"(",")",")",["T_WHITESPACE"," ",179],"{",["T_WHITESPACE","\n      ",179],["T_VARIABLE","$grants",180],"[","]",["T_WHITESPACE"," ",180],"=",["T_WHITESPACE"," ",180],["T_ARRAY","array",180],"(",["T_WHITESPACE","\n        ",180],["T_CONSTANT_ENCAPSED_STRING","'realm'",181],["T_WHITESPACE"," ",181],["T_DOUBLE_ARROW","=>",181],["T_WHITESPACE"," ",181],["T_CONSTANT_ENCAPSED_STRING","'example_author'",181],",",["T_WHITESPACE","\n        ",181],["T_CONSTANT_ENCAPSED_STRING","'gid'",182],["T_WHITESPACE"," ",182],["T_DOUBLE_ARROW","=>",182],["T_WHITESPACE"," ",182],["T_VARIABLE","$node",182],["T_OBJECT_OPERATOR","->",182],["T_STRING","getOwnerId",182],"(",")",",",["T_WHITESPACE","\n        ",182],["T_CONSTANT_ENCAPSED_STRING","'grant_view'",183],["T_WHITESPACE"," ",183],["T_DOUBLE_ARROW","=>",183],["T_WHITESPACE"," ",183],["T_LNUMBER","1",183],",",["T_WHITESPACE","\n        ",183],["T_CONSTANT_ENCAPSED_STRING","'grant_update'",184],["T_WHITESPACE"," ",184],["T_DOUBLE_ARROW","=>",184],["T_WHITESPACE"," ",184],["T_LNUMBER","1",184],",",["T_WHITESPACE","\n        ",184],["T_CONSTANT_ENCAPSED_STRING","'grant_delete'",185],["T_WHITESPACE"," ",185],["T_DOUBLE_ARROW","=>",185],["T_WHITESPACE"," ",185],["T_LNUMBER","1",185],",",["T_WHITESPACE","\n        ",185],["T_CONSTANT_ENCAPSED_STRING","'langcode'",186],["T_WHITESPACE"," ",186],["T_DOUBLE_ARROW","=>",186],["T_WHITESPACE"," ",186],["T_CONSTANT_ENCAPSED_STRING","'ca'",186],["T_WHITESPACE","\n      ",186],")",";",["T_WHITESPACE","\n    ",187],"}",["T_WHITESPACE","\n\n    ",188],["T_RETURN","return",190],["T_WHITESPACE"," ",190],["T_VARIABLE","$grants",190],";",["T_WHITESPACE","\n  ",190],"}",["T_WHITESPACE","\n",191],"}",["T_WHITESPACE","\n\n",192],["T_DOC_COMMENT","\/**\n * Alter permissions for a node before it is written to the database.\n *\n * Node access modules establish rules for user access to content. Node access\n * records are stored in the {node_access} table and define which permissions\n * are required to access a node. This hook is invoked after node access modules\n * returned their requirements via hook_node_access_records(); doing so allows\n * modules to modify the $grants array by reference before it is stored, so\n * custom or advanced business logic can be applied.\n *\n * Upon viewing, editing or deleting a node, hook_node_grants() builds a\n * permissions array that is compared against the stored access records. The\n * user must have one or more matching permissions in order to complete the\n * requested operation.\n *\n * A module may deny all access to a node by setting $grants to an empty array.\n *\n * The preferred use of this hook is in a module that bridges multiple node\n * access modules with a configurable behavior, as shown in the example with the\n * 'is_preview' field.\n *\n * @param array $grants\n *   The $grants array returned by hook_node_access_records().\n * @param \\Drupal\\node\\NodeInterface $node\n *   The node for which the grants were acquired.\n *\n * @see hook_node_access_records()\n * @see hook_node_grants()\n * @see hook_node_grants_alter()\n * @ingroup node_access\n *\/",194],["T_WHITESPACE","\n",224],["T_FUNCTION","function",225],["T_WHITESPACE"," ",225],["T_STRING","hook_node_access_records_alter",225],"(","&",["T_VARIABLE","$grants",225],",",["T_WHITESPACE"," ",225],["T_STRING","Drupal",225],["T_NS_SEPARATOR","\\",225],["T_STRING","node",225],["T_NS_SEPARATOR","\\",225],["T_STRING","NodeInterface",225],["T_WHITESPACE"," ",225],["T_VARIABLE","$node",225],")",["T_WHITESPACE"," ",225],"{",["T_WHITESPACE","\n  ",225],["T_COMMENT","\/\/ Our module allows editors to mark specific articles with the 'is_preview'\n",226],["T_WHITESPACE","  ",227],["T_COMMENT","\/\/ field. If the node being saved has a TRUE value for that field, then only\n",227],["T_WHITESPACE","  ",228],["T_COMMENT","\/\/ our grants are retained, and other grants are removed. Doing so ensures\n",228],["T_WHITESPACE","  ",229],["T_COMMENT","\/\/ that our rules are enforced no matter what priority other grants are given.\n",229],["T_WHITESPACE","  ",230],["T_IF","if",230],["T_WHITESPACE"," ",230],"(",["T_VARIABLE","$node",230],["T_OBJECT_OPERATOR","->",230],["T_STRING","is_preview",230],")",["T_WHITESPACE"," ",230],"{",["T_WHITESPACE","\n    ",230],["T_COMMENT","\/\/ Our module grants are set in $grants['example'].\n",231],["T_WHITESPACE","    ",232],["T_VARIABLE","$temp",232],["T_WHITESPACE"," ",232],"=",["T_WHITESPACE"," ",232],["T_VARIABLE","$grants",232],"[",["T_CONSTANT_ENCAPSED_STRING","'example'",232],"]",";",["T_WHITESPACE","\n    ",232],["T_COMMENT","\/\/ Now remove all module grants but our own.\n",233],["T_WHITESPACE","    ",234],["T_VARIABLE","$grants",234],["T_WHITESPACE"," ",234],"=",["T_WHITESPACE"," ",234],["T_ARRAY","array",234],"(",["T_CONSTANT_ENCAPSED_STRING","'example'",234],["T_WHITESPACE"," ",234],["T_DOUBLE_ARROW","=>",234],["T_WHITESPACE"," ",234],["T_VARIABLE","$temp",234],")",";",["T_WHITESPACE","\n  ",234],"}",["T_WHITESPACE","\n",235],"}",["T_WHITESPACE","\n\n",236],["T_DOC_COMMENT","\/**\n * Alter user access rules when trying to view, edit or delete a node.\n *\n * Node access modules establish rules for user access to content.\n * hook_node_grants() defines permissions for a user to view, edit or delete\n * nodes by building a $grants array that indicates the permissions assigned to\n * the user by each node access module. This hook is called to allow modules to\n * modify the $grants array by reference, so the interaction of multiple node\n * access modules can be altered or advanced business logic can be applied.\n *\n * The resulting grants are then checked against the records stored in the\n * {node_access} table to determine if the operation may be completed.\n *\n * A module may deny all access to a user by setting $grants to an empty array.\n *\n * Developers may use this hook to either add additional grants to a user or to\n * remove existing grants. These rules are typically based on either the\n * permissions assigned to a user role, or specific attributes of a user\n * account.\n *\n * @param array $grants\n *   The $grants array returned by hook_node_grants().\n * @param \\Drupal\\Core\\Session\\AccountInterface $account\n *   The account requesting access to content.\n * @param string $op\n *   The operation being performed, 'view', 'update' or 'delete'.\n *\n * @see hook_node_grants()\n * @see hook_node_access_records()\n * @see hook_node_access_records_alter()\n * @ingroup node_access\n *\/",238],["T_WHITESPACE","\n",269],["T_FUNCTION","function",270],["T_WHITESPACE"," ",270],["T_STRING","hook_node_grants_alter",270],"(","&",["T_VARIABLE","$grants",270],",",["T_WHITESPACE"," ",270],["T_NS_SEPARATOR","\\",270],["T_STRING","Drupal",270],["T_NS_SEPARATOR","\\",270],["T_STRING","Core",270],["T_NS_SEPARATOR","\\",270],["T_STRING","Session",270],["T_NS_SEPARATOR","\\",270],["T_STRING","AccountInterface",270],["T_WHITESPACE"," ",270],["T_VARIABLE","$account",270],",",["T_WHITESPACE"," ",270],["T_VARIABLE","$op",270],")",["T_WHITESPACE"," ",270],"{",["T_WHITESPACE","\n  ",270],["T_COMMENT","\/\/ Our sample module never allows certain roles to edit or delete\n",271],["T_WHITESPACE","  ",272],["T_COMMENT","\/\/ content. Since some other node access modules might allow this\n",272],["T_WHITESPACE","  ",273],["T_COMMENT","\/\/ permission, we expressly remove it by returning an empty $grants\n",273],["T_WHITESPACE","  ",274],["T_COMMENT","\/\/ array for roles specified in our variable setting.\n",274],["T_WHITESPACE","\n  ",275],["T_COMMENT","\/\/ Get our list of banned roles.\n",276],["T_WHITESPACE","  ",277],["T_V