[["T_OPEN_TAG","<?php\n",1],["T_DOC_COMMENT","\/**\n * Proxy connection interface\n *\n * @package Requests\n * @subpackage Proxy\n * @since 1.6\n *\/",2],["T_WHITESPACE","\n\n",8],["T_DOC_COMMENT","\/**\n * Proxy connection interface\n *\n * Implement this interface to handle proxy settings and authentication\n *\n * Parameters should be passed via the constructor where possible, as this\n * makes it much easier for users to use your provider.\n *\n * @see Requests_Hooks\n * @package Requests\n * @subpackage Proxy\n * @since 1.6\n *\/",10],["T_WHITESPACE","\n",22],["T_INTERFACE","interface",23],["T_WHITESPACE"," ",23],["T_STRING","Requests_Proxy",23],["T_WHITESPACE"," ",23],"{",["T_WHITESPACE","\n\t",23],["T_DOC_COMMENT","\/**\n\t * Register hooks as needed\n\t *\n\t * This method is called in {@see Requests::request} when the user has set\n\t * an instance as the 'auth' option. Use this callback to register all the\n\t * hooks you'll need.\n\t *\n\t * @see Requests_Hooks::register\n\t * @param Requests_Hooks $hooks Hook system\n\t *\/",24],["T_WHITESPACE","\n\t",33],["T_PUBLIC","public",34],["T_WHITESPACE"," ",34],["T_FUNCTION","function",34],["T_WHITESPACE"," ",34],["T_STRING","register",34],"(",["T_STRING","Requests_Hooks",34],["T_WHITESPACE"," ",34],"&",["T_VARIABLE","$hooks",34],")",";",["T_WHITESPACE","\n",34],"}"]