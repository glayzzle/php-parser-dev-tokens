[["T_OPEN_TAG","<?php\n",1],["T_DOC_COMMENT","\/**\n * Authentication provider interface\n *\n * @package Requests\n * @subpackage Authentication\n *\/",2],["T_WHITESPACE","\n\n",7],["T_DOC_COMMENT","\/**\n * Authentication provider interface\n *\n * Implement this interface to act as an authentication provider.\n *\n * Parameters should be passed via the constructor where possible, as this\n * makes it much easier for users to use your provider.\n *\n * @see Requests_Hooks\n * @package Requests\n * @subpackage Authentication\n *\/",9],["T_WHITESPACE","\n",20],["T_INTERFACE","interface",21],["T_WHITESPACE"," ",21],["T_STRING","Requests_Auth",21],["T_WHITESPACE"," ",21],"{",["T_WHITESPACE","\n\t",21],["T_DOC_COMMENT","\/**\n\t * Register hooks as needed\n\t *\n\t * This method is called in {@see Requests::request} when the user has set\n\t * an instance as the 'auth' option. Use this callback to register all the\n\t * hooks you'll need.\n\t *\n\t * @see Requests_Hooks::register\n\t * @param Requests_Hooks $hooks Hook system\n\t *\/",22],["T_WHITESPACE","\n\t",31],["T_PUBLIC","public",32],["T_WHITESPACE"," ",32],["T_FUNCTION","function",32],["T_WHITESPACE"," ",32],["T_STRING","register",32],"(",["T_STRING","Requests_Hooks",32],["T_WHITESPACE"," ",32],"&",["T_VARIABLE","$hooks",32],")",";",["T_WHITESPACE","\n",32],"}"]