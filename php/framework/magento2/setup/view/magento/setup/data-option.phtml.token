[["T_OPEN_TAG","<?php\n",1],["T_DOC_COMMENT","\/**\n * Copyright \u00c2\u00a9 2013-2017 Magento, Inc. All rights reserved.\n * See COPYING.txt for license details.\n *\/",2],["T_WHITESPACE","\n",5],["T_CLOSE_TAG","?>\n",6],["T_INLINE_HTML","\n<div class=\"nav-bar-outer-actions\">\n    <div class=\"outer-actions-inner-wrap\">\n        <div class=\"btn-wrap btn-wrap-triangle-right btn-wrap-next\">\n            <button\n                type=\"button\"\n                class=\"btn btn-prime\"\n                ng-click=\"nextState()\"\n                >Next<\/button>\n        <\/div>\n        <div class=\"btn-wrap btn-wrap-triangle-left btn-wrap-prev\">\n            <button\n                type=\"button\"\n                class=\"btn\"\n                ng-click=\"goToBackup()\"\n                >Back<\/button>\n        <\/div>\n    <\/div>\n<\/div>\n\n<h2 class=\"page-title\">{{$state.current.header}}<\/h2>\n\n<div ng-controller=\"dataOptionController\">\n    <div class=\"message\" ng-hide=\"component.hasUninstall\">\n        The selected component has no data to remove.\n    <\/div>\n    <div ng-show=\"component.hasUninstall\">\n        <div class=\"form-row\">\n            What do you want to do with {{moduleName}}'s data?<br\/>\n            <\/div>\n        <div class=\"form-row\">\n            <input\n                id=\"keepData\"\n                class=\"form-el-radio\"\n                type=\"radio\"\n                ng-model=\"component.dataOption\"\n                value=\"false\"\n                >\n            <label class=\"form-label\" for=\"keepData\">\n                <b>Keep data<\/b>\n                <br\/><br\/>\n                Note: You can see the associated data in your database,\n                however your customer will not see this in your online store.\n            <\/label>\n\n        <\/div>\n        <div class=\"form-row\">\n            <input\n                id=\"removeData\"\n                class=\"form-el-radio\"\n                type=\"radio\"\n                ng-model=\"component.dataOption\"\n                value=\"true\"\n                >\n            <label class=\"form-label\" for=\"removeData\">\n                <b>Remove data<\/b>\n                <br\/><br\/>\n                Did you back up first? Choosing to remove data means the data is unavailable unless you\n                recover it from a backup in the preceding step.\n            <\/label>\n        <\/div>\n    <\/div>\n<\/div>\n",7]]